{
  "name": "Shipping",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "8.0.0"
  },
  "scripts": {
    "start": "node ./server",
    "watch": "./node_modules/.bin/nodemon ./server",
    "test-units": "mocha ./test/unit/**/*.spec.js",
    "test-database": "export NODE_ENV=test; yarn migrate-refresh; mocha ./test/database/**/*.spec.js",
    "test-api": "export NODE_ENV=test; yarn migrate-refresh; mocha ./test/api/*.spec.js",
    "migrate-refresh": "sequelize db:migrate:undo:all; sequelize db:migrate; sequelize db:seed:all",
    "lint": "./node_modules/.bin/eslint",
    "sequelize": "./node_modules/.bin/sequelize"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*"
    ],
    "delay": "2500"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "debug": "~2.2.0",
    "dotenv": "^4.0.0",
    "express": "~4.14.0",
    "log": "^1.4.0",
    "mocha": "^3.2.0",
    "morgan": "~1.7.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.37.10"
  },
  "devDependencies": {
    "eslint": "^4.19.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "nodemon": "^1.17.2"
  }
}
